  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^4.0.0",
    "body-parser": "^1.18.2",
    "clean-webpack-plugin": "^0.1.17",
    "codecov": "^2.3.1",
    "eslint": "^4.9.0",
    "express": "^4.16.2",
    "firebase-tools": "^3.13.1",
    "flow-bin": "^0.57.3",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.5",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "react": "16.0.0-alpha.12",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-native": "0.46.4",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1",
    "webpack-build-notifier": "^0.1.16",
    "webpack-dev-server": "^2.9.3",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.0"
  },
  "scripts": {
    "start:web:dev": "./node_modules/.bin/webpack-dev-server --env=dev --progress --profile --colors --open",
    "flow": "./node_modules/.bin/flow",
    "lint": "./node_modules/.bin/eslint .",
    "test": "karma start",
    "prod": "./node_modules/.bin/webpack --env=prod",
    "report-coverage": "codecov -t $CODECOV_KEY",
    "firebase-deploy": "./node_modules/.bin/firebase deploy",
    "firebase": "yarn flow && yarn lint && yarn test && yarn report-coverage && yarn prod && yarn firebase-deploy",
    "travis": "yarn firebase",
    "start": "npm run prod && node server/server",
    "cleanup": "./node_modules/.bin/rimraf android/app/src/main/assets ./.prod ./.dist ./.coverage TMPDIR/react-* android ios node_modules && yarn",
    "eject": "react-native eject",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "bundle:mobile": "yarn cleanup && yarn eject && mkdir android/app/src/main/assets && yarn bundle && yarn cleanup",
    "start:mobile:server": "node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "start:mobile:dev": "react-native run-android",
    "start:mobile:dev:ios": "react-native run-ios",
    "test:mobile": "./node_modules/.bin/jest"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/tests/**/*.js?(x)"
    ]
  },
  "engines": {
    "node": "7.6.0",
    "npm": "4.1.2"
  }
}
